createToggle("Auto Hunt Legendary", "AutoLegendary", Color3.fromRGB(255, 140, 0), Color3.fromRGB(255, 69, 0))
createToggle("Auto Lụm Tiền", "AutoCollectMoney", Color3.fromRGB(255, 165, 0), Color3.fromRGB(255, 255, 0))

createSpeedButton("Bơi Nhanh x200", 200, Color3.fromRGB(148, 0, 211))  -- Tím đậm cho x200

